<% if @feed_items.any? %>
  <ol class="todos">
  	<!-- pass a :collection parameter with the feed items, which causes render to use the given partial (’feed_item’ in this case) to render each item in the collection. 

  	Omitting partial: here doesn't work as collection: is used-->
    <%= render partial: 'shared/feed_item', collection: @feed_items %>
  </ol>
  <%= will_paginate @feed_items %>
<% end %>